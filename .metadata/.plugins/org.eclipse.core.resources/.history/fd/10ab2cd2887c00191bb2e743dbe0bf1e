package jp.co.internous.action;

public class Person4 {

	public String name = null;
	public int age = 0;

	public Person4 (String name,int age){

		this.name = name;
		this.age = age;

	}

	public String getName(){

		return this.name;

	}

	public void stName(String name){

		this.name = name;

	}

}
